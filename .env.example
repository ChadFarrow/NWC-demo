# Alby OAuth Credentials (Development)
ALBY_ID_5173=your_alby_client_id_here
ALBY_SECRET_5173=your_alby_client_secret_here
ALBY_JWT=your_jwt_secret_here

# Alby Access Token (for API fallback)
ALBY_ACCESS_TOKEN=your_alby_access_token_here

# NWC Connection String
NWC_CONNECTION_STRING=nostr+walletconnect://wallet_pubkey?relay=wss://relay.getalby.com/v1&secret=your_secret&lud16=your_lightning_address

# Webhook Secret (for Alby webhook verification)
WEBHOOK_SECRET=your_webhook_secret_here

# Environment
NODE_ENV=production