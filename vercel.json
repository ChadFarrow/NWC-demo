{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.js",
      "use": "@vercel/static"
    },
    {
      "src": "*.xml",
      "use": "@vercel/static"
    },
    {
      "src": "*.png",
      "use": "@vercel/static"
    },
    {
      "src": "*.css",
      "use": "@vercel/static"
    },
    {
      "src": "*.txt",
      "use": "@vercel/static"
    },
    {
      "src": "*.json",
      "use": "@vercel/static"
    },
    {
      "src": "src/**/*",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    { "source": "/", "destination": "/public/v4v_lightning_tester.html" },
    { "source": "/dashboard.html", "destination": "/public/dashboard.html" },
    { "source": "/dashboard", "destination": "/public/dashboard.html" },
    { "source": "/metaboost-dashboard", "destination": "/public/metaboost-dashboard.html" },
    { "source": "/metaboost-dashboard.html", "destination": "/public/metaboost-dashboard.html" },
    { "source": "/test", "destination": "/public/test.html" },
    { "source": "/test.html", "destination": "/public/test.html" },
    { "source": "/test-bridge", "destination": "/public/test-bridge.html" },
    { "source": "/test-bridge.html", "destination": "/public/test-bridge.html" },
    { "source": "/test-direct-nwc", "destination": "/public/test-direct-nwc.html" },
    { "source": "/test-direct-nwc.html", "destination": "/public/test-direct-nwc.html" },
    { "source": "/test-simple", "destination": "/public/test-simple.html" },
    { "source": "/test-simple.html", "destination": "/public/test-simple.html" },
    { "source": "/test-keysend-fix", "destination": "/test-keysend-fix.html" },
    { "source": "/test-keysend-fix.html", "destination": "/test-keysend-fix.html" },
    { "source": "/test-nwc-standalone", "destination": "/test-nwc-standalone.html" },
    { "source": "/test-nwc-standalone.html", "destination": "/test-nwc-standalone.html" },
    { "source": "/test-splits", "destination": "/test-splits.html" },
    { "source": "/test-splits.html", "destination": "/test-splits.html" },
    { "source": "/dual-payment-demo", "destination": "/public/dual-payment-demo.html" },
    { "source": "/dual-payment-demo.html", "destination": "/public/dual-payment-demo.html" },
    { "source": "/api/metaboost", "destination": "/api/metaboost.js" },
    { "source": "/api/metaboost-viewer", "destination": "/api/metaboost-viewer.js" },
    { "source": "/api/metaboost-v2", "destination": "/api/metaboost-v2.js" },
    { "source": "/api/dashboard", "destination": "/api/dashboard.js" },
    { "source": "/api/nwc/keysend", "destination": "/api/nwc-keysend.js" },
    { "source": "/api/proxy/keysend", "destination": "/api/proxy-keysend.js" },
    { "source": "/api/proxy/keysend-real", "destination": "/api/proxy-keysend-real.js" },
    { "source": "/api/test-nwc", "destination": "/api/test-nwc.js" },
    { "source": "/api/wallet-info", "destination": "/api/wallet-info.js" },
    { "source": "/api/metaboost-dashboard", "destination": "/api/metaboost-dashboard.js" },
    { "source": "/favicon.ico", "destination": "/public/favicon.png" },
    { "source": "/logo", "destination": "/public/logo.png" }
  ],
  "headers": [
    {
      "source": "/(.*\\.js)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript"
        }
      ]
    },
    {
      "source": "/(.*\\.xml)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/xml"
        }
      ]
    },
    {
      "source": "/(.*\\.html)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/html"
        }
      ]
    },
    {
      "source": "/(.*\\.css)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/css"
        }
      ]
    }
  ]
}